<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <display-name>Lab9</display-name>
    <servlet>
        <servlet-name>Aaa</servlet-name>
        <servlet-class>servlets.Aaa</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Bbb</servlet-name>
        <servlet-class>servlets.Bbb</servlet-class>
    </servlet>
        <servlet>
        <servlet-name>Ccc</servlet-name>
        <servlet-class>servlets.Ccc</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Aaa</servlet-name>
        <url-pattern>/Aaa</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Bbb</servlet-name>
        <url-pattern>/Bbb</url-pattern>
    </servlet-mapping>
        <servlet-mapping>
        <servlet-name>Ccc</servlet-name>
        <url-pattern>/Ccc</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>