<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <display-name>Lab11</display-name>
    <servlet>
        <servlet-name>Sss_Header</servlet-name>
        <servlet-class>lab11.Sss_Header</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Sss_Header</servlet-name>
        <url-pattern>/Sss_Header</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Sss_Xml</servlet-name>
        <servlet-class>lab11.Sss_Xml</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Sss_Xml</servlet-name>
        <url-pattern>/Sss_Xml</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Sss_Json</servlet-name>
        <servlet-class>lab11.Sss_Json</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Sss_Json</servlet-name>
        <url-pattern>/Sss_Json</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>